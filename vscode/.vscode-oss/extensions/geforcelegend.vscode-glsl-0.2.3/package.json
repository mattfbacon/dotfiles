{
	"name": "vscode-glsl",
	"displayName": "GLSL Syntax for VS Code",
	"description": "Provides GLSL(OpenGL Shading Language) syntax for Visual Studio Code",
	"version": "0.2.3",
	"publisher": "GeForceLegend",
	"categories": [
		"Snippets",
		"Programming Languages"
	],
	"license": "LICENSE",
	"icon": "icon.png",
	"bugs": {
		"url": "https://github.com/GeForceLegend/vscode-glsl/issues"
	},
	"engines": {
		"vscode": "^1.53.0"
	},
	"homepage": "https://github.com/GeForceLegend/vscode-glsl",
	"repository": {
		"type": "git",
		"url": "https://github.com/GeForceLegend/vscode-glsl.git"
	},
	"keywords": [
		"GLSL",
		"glsl",
		"OpenGL",
		"shader"
	],
	"activationEvents": [
		"onLanguage:glsl"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [
			{
				"id": "glsl",
				"aliases": [
					"OpenGL Shading Language",
					"GLSL",
					"glsl"
				],
				"extensions": [
					".vs",
					".fs",
					".gs",
					".vsh",
					".fsh",
					".gsh",
					".csh",
					".vshader",
					".fshader",
					".gshader",
					".vert",
					".frag",
					".geom",
					".tesc",
					".tese",
					".comp",
					".glsl"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "glsl",
				"scopeName": "source.glsl",
				"path": "./syntaxes/glsl.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/node": "^12.12.0",
		"@types/vscode": "^1.42.0",
		"@typescript-eslint/eslint-plugin": "^4.16.0",
		"@typescript-eslint/parser": "^4.16.0",
		"eslint": "^7.21.0",
		"typescript": "^4.2.2"
	},
	"__metadata": {
		"id": "e0d0964c-f2c7-4447-98a8-8ac8ddb196a5",
		"publisherId": "4fb38176-136a-4943-9f79-eb4d54eeb20a",
		"publisherDisplayName": "GeForceLegend",
		"isPreReleaseVersion": false,
		"preRelease": false,
		"installedTimestamp": 1647359099407
	}
}