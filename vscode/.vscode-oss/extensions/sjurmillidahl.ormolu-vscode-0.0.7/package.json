{
	"name": "ormolu-vscode",
	"displayName": "ormolu",
	"version": "0.0.7",
	"repository": {
		"type": "git",
		"url": "https://github.com/mariatsji/ormolu-vscode"
	},
	"icon": "images/icon.png",
	"description": "VS Code extension for Haskell code formatting based on ormolu tool",
	"engines": {
		"vscode": "^1.39.2"
	},
	"categories": [
		"Formatters"
	],
	"activationEvents": [
		"onLanguage:haskell"
	],
	"contributes": {
		"languages": [
			{
				"id": "haskell"
			}
		],
		"configuration": {
			"title": "Ormolu",
			"type": "object",
			"properties": {
				"ormolu.notifyOnParseError": {
					"type": "boolean",
					"default": false,
					"description": "If set to true, notify you every time ormolu fails to parse a file"
				},
				"ormolu.path": {
					"type": "string",
					"default": "ormolu",
					"description": "Path to the ormolu executable."
				}
			}
		}
	},
	"scripts": {
		"compile": "tsc",
		"postinstall": "node ./node_modules/vscode/bin/install && tsc"
	},
	"devDependencies": {
		"@types/node": "*",
		"typescript": "3.9.7",
		"vscode": "^1.1.37"
	},
	"main": "./out/src/extension",
	"author": "Sjur Millidahl",
	"publisher": "sjurmillidahl",
	"license": "MIT",
	"private": false,
	"__metadata": {
		"id": "0b044e66-6327-4b2e-a162-85bb2f3d6108",
		"publisherId": "4a18b6ca-986a-424d-93a9-c693b9598c6c",
		"publisherDisplayName": "sjurmillidahl",
		"isPreReleaseVersion": false,
		"preRelease": false,
		"installedTimestamp": 1647359004613
	}
}