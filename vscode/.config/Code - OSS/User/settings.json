		"memmap",
		"Minecart",
		"minecraft",
		"Mmap",
		"Mojang",
		"msgpack",
		"multer",
		"nacks",
		"NOLINT",
		"NOLINTNEXTLINE",
		"nonblocking",
		"NTFS",
		"oneshot",
		"optimizable",
		"ormx",
		"Overworld",
		"pango",
		"pangocairo",
		"peekable",
		"pkcs",
		"previewable",
		"relayout",
		"Renderable",
		"repr",
		"reqwest",
		"respawn",
		"rfold",
		"rsplit",
		"rusqlite",
		"serde",
		"Shulker",
		"Smithing",
		"sqlx",
		"stringly",
		"struct",
		"structs",
		"subprojects",
		"Tempfile",
		"thiserror",
		"traceback",
		"Tweakable",
		"underflowed",
		"Uninit",
		"unoptimized",
		"Unprefixed",
		"unseparated",
		"unshuffled",
		"untagging",
		"varint",
		"vendoring",
		"viewspec"
	],
	"ccls.cache.directory": "/home/matt/.cache/ccls/${workspaceFolder}",
	"ccls.codeLens.enabled": false,
	"debug.showInlineBreakpointCandidates": false,
	"editor.autoSurround": "never",
	"editor.bracketPairColorization.enabled": false,
	"editor.cursorStyle": "line",
	"editor.emptySelectionClipboard": false,
	"editor.fontFamily": "monospace",
	"editor.fontLigatures": true,
	"editor.fontSize": 13,
	"editor.fontWeight": 480,
	"editor.formatOnSave": true,
	"editor.glyphMargin": false,
	"editor.insertSpaces": false,
	"editor.lineHeight": 1.5,
	"editor.lineNumbers": "relative",
	"editor.occurrencesHighlight": false,
	"editor.quickSuggestions": {
		"comments": "off",
		"other": "off",
		"strings": "off"
	},
	"editor.renderWhitespace": "boundary",
	"editor.rulers": [
		79,
		160
	],
	"editor.suggest.localityBonus": true,
	"editor.tabSize": 2,
	"editor.tokenColorCustomizations": {
		"textMateRules": [
			{
				"scope": "heading.1.markdown entity.name.section.markdown, heading.1.markdown punctuation.definition.heading.markdown",
				"settings": {
					"foreground": "#e66767"
				}
			},
			{
				"scope": "heading.2.markdown entity.name.section.markdown, heading.2.markdown punctuation.definition.heading.markdown",
				"settings": {
					"foreground": "#f19066"
				}
			},
			{
				"scope": "heading.3.markdown entity.name.section.markdown, heading.3.markdown punctuation.definition.heading.markdown",
				"settings": {
					"foreground": "#f6b93b"
				}
			},
			{
				"scope": "heading.4.markdown entity.name.section.markdown, heading.4.markdown punctuation.definition.heading.markdown",
				"settings": {
					"foreground": "#78e08f"
				}
			},
			{
				"scope": "heading.5.markdown entity.name.section.markdown, heading.5.markdown punctuation.definition.heading.markdown",
				"settings": {
					"foreground": "#546de5"
				}
			},
			{
				"scope": "heading.6.markdown entity.name.section.markdown, heading.6.markdown punctuation.definition.heading.markdown",
				"settings": {
					"foreground": "#574b90"
				}
			}
		]
	},
	"editor.unicodeHighlight.includeComments": true,
	"editor.unicodeHighlight.nonBasicASCII": true,
	"editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
	"editor.wordWrap": "on",
	"editor.wrappingIndent": "indent",
	"emmet.syntaxProfiles": {
		"c++": {
			"Quote Include": {
				"body": "#include \"$1\"",
				"description": "Include local header",
				"prefix": "i\""
			},
			"System Include": {
				"body": "#include <$1>",
				"description": "Include system header",
				"prefix": "i<"
			}
		}
	},
	"eslint.format.enable": true,
	"eslint.packageManager": "yarn",
	"eslint.validate": [
		"typescript",
		"javascript"
	],
	"evenBetterToml.formatter.allowedBlankLines": 1,
	"evenBetterToml.formatter.columnWidth": 120,
	"evenBetterToml.formatter.reorderKeys": true,
	"files.associations": {
		"*.ejs": "plaintext",
		"*.h": "cpp",
		"*.html": "html",
		"*.make": "makefile",
		"*.qss": "css",
		"*.tpp": "cpp",
		".htmlhintrc": "json",
		"TODO": "markdown"
	},
	"files.exclude": {
		"**/.classpath": true,
		"**/.factorypath": true,
		"**/.git": false,
		"**/.project": true,
		"**/.settings": true
	},
	"files.hotExit": "off",
	"files.insertFinalNewline": true,
	"files.trimFinalNewlines": true,
	"git.autofetch": true,
	"gitlens.advanced.abbreviatedShaLength": 8,
	"gitlens.advanced.messages": {
		"suppressGitDisabledWarning": true
	},
	"gitlens.codeLens.enabled": false,
	"gitlens.statusBar.enabled": false,
	"haskell.serverExecutablePath": "~/.ghcup/bin/haskell-language-server-8.10.7",
	"html.autoClosingTags": false,
	"html.autoCreateQuotes": false,
	"html.format.endWithNewline": true,
	"java.import.gradle.java.home": "/usr/lib/jvm/android-studio",
	"javascript.autoClosingTags": false,
	"javascript.suggest.autoImports": false,
	"json.schemaDownload.enable": false,
	"less.lint.duplicateProperties": "warning",
	"less.lint.float": "warning",
	"less.lint.importStatement": "error",
	"less.lint.important": "warning",
	"less.lint.unknownAtRules": "error",
	"less.lint.unknownProperties": "error",
	"less.lint.zeroUnits": "warning",
	"python.formatting.provider": "yapf",
	"python.languageServer": "Jedi",
	"python.linting.pylintEnabled": false,
	"python.terminal.activateEnvironment": false,
	"redhat.telemetry.enabled": false,
	"rpc.detailsEditing": "In {workspace}",
	"rpc.detailsViewing": "In {workspace}",
	"rpc.fileInfoText": "{language} file ({currentline})",
	"rpc.lowerDetailsEditing": "Editing {file_name}",
	"rpc.lowerDetailsNoWorkspaceFound": "{current_line}/{total_lines}",
	"rpc.placeholderText": "VSCodium",
	"rpc.placeholderTextInsiders": "VSCodium Insiders",
	"rpc.removeElapsedTime": true,
	"rpc.showProblems": false,
	"rpc.showTime": false,
	"rust-analyzer.cargo.features": "all",
	"rust-analyzer.checkOnSave.command": "clippy",
	"rust-analyzer.inlayHints.bindingModeHints.enable": true,
	"rust-analyzer.inlayHints.closureReturnTypeHints.enable": "with_block",
	"rust-analyzer.inlayHints.lifetimeElisionHints.enable": "always",
	"rust-analyzer.inlayHints.reborrowHints.enable": "always",
	"rust-analyzer.inlayHints.typeHints.hideClosureInitialization": true,
	"rust-analyzer.rustfmt.extraArgs": [
		"+nightly"
	],
	"rust.clippy_preference": "on",
	"rust.jobs": 32,
	"rust.rust-analyzer.path": "/usr/bin/rust-analyzer",
	"rust.rust-analyzer.releaseTag": "stable",
	"security.workspace.trust.enabled": false,
	"security.workspace.trust.untrustedFiles": "open",
	"settingsSync.keybindingsPerPlatform": false,
	"sundial.sunrise": "06:30",
	"sundial.sunset": "21:00",
	"terminal.external.linuxExec": "kitty",
	"terminal.integrated.allowChords": false,
	"terminal.integrated.allowMnemonics": false,
	"terminal.integrated.altClickMovesCursor": true,
	"terminal.integrated.automationProfile.linux": {
		"path": "/usr/bin/bash"
	},
	"terminal.integrated.automationShell.linux": "bash",
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.defaultProfile.linux": "fish",
	"terminal.integrated.drawBoldTextInBrightColors": false,
	"terminal.integrated.env.linux": {
		"GIT_DIFFTOOL": "vscodium --wait --diff $LOCAL $REMOTE",
		"GIT_EDITOR": "vscodium --wait",
		"GIT_MERGETOOL": "vscodium --wait $MERGED"
	},
	"terminal.integrated.fastScrollSensitivity": 5,
	"terminal.integrated.fontSize": 13,
	"terminal.integrated.fontWeight": 480,
	"terminal.integrated.fontWeightBold": 700,
	"terminal.integrated.gpuAcceleration": "on",
	"terminal.integrated.lineHeight": 1.1,
	"terminal.integrated.profiles.linux": {
		"fish": {
			"env": {
				"GIT_DIFFTOOL": "vscodium --wait --diff $LOCAL $REMOTE",
				"GIT_EDITOR": "vscodium --wait",
				"GIT_MERGETOOL": "vscodium --wait $MERGED"
			},
			"icon": "star-full",
			"overrideName": true,
			"path": "/usr/bin/fish"
		}
	},
	"terminal.integrated.sendKeybindingsToShell": true,
	"typescript.autoClosingTags": false,
	"typescript.format.enable": false,
	"update.mode": "none",
	"update.showReleaseNotes": false,
	"vscode-neovim.neovimExecutablePaths.linux": "/usr/bin/nvim",
	"vscode-neovim.neovimInitVimPaths.linux": "~/.config/nvim/init.vim",
	"window.menuBarVisibility": "toggle",
	"window.zoomLevel": -1,
	"workbench.colorTheme": "MattTheme Light",
	"workbench.editor.tabSizing": "shrink",
	"workbench.editorAssociations": {
		"*.ipynb": "jupyter.notebook.ipynb",
		"*.ts": "default"
	},
	"workbench.enableExperiments": false,
	"workbench.iconTheme": "file-icons-colourless",
	"workbench.preferredDarkColorTheme": "MattTheme Dark",
	"workbench.preferredLightColorTheme": "MattTheme Light",
	"workbench.productIconTheme": "fluent-icons",
	"workbench.settings.enableNaturalLanguageSearch": false,
	"workbench.startupEditor": "newUntitledFile",
	"workbench.tree.indent": 7,
	"workbench.tree.renderIndentGuides": "always",
	"xml.completion.autoCloseRemovesContent": false,
	"xml.completion.autoCloseTags": false
}
