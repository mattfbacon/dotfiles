[core]
	editor = nvim
	sshCommand = ssh -i ~/.ssh/id_rsa -F /dev/null
	excludesfile = ~/.gitignore
[pull]
	rebase = false
[bash]
	showInformativeStatus = true
[diff]
	colorMoved = true
	algorithm = histogram
[push]
	default = matching
[init]
	defaultBranch = main
[alias]
	tree = log --graph --oneline --all
	amend-to = "!f() { SHA=`git rev-parse \"$1\"`; git stash -k && git commit --fixup \"$SHA\" && GIT_SEQUENCE_EDITOR=true git rebase --interactive --autosquash \"$SHA^\" && git stash pop; }; f"
[commit]
	gpgsign = true
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
